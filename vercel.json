{
    "version": 2,
    "builds": [
        {
            "src": "gateway/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "api-berita-indonesia/src/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "cnnindonesia-news-api/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "detiknews_api/main.py",
            "use": "@vercel/python"
        },
        {
            "src": "berita-indo-api/package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/cnn-api/(.*)",
            "dest": "/cnnindonesia-news-api/main.py"
        },
        {
            "src": "/detik/(.*)",
            "dest": "/detiknews_api/main.py"
        },
        {
            "src": "/rss/(.*)",
            "dest": "/api-berita-indonesia/src/server.js"
        },
        {
            "src": "/berita-indo/(.*)",
            "dest": "/berita-indo-api/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "/gateway/server.js"
        },
        {
            "src": "/",
            "dest": "/gateway/server.js"
        }
    ]
}