{
    "version": 2,
    "functions": {
        "cnnindonesia-news-api/main.py": {
            "runtime": "@vercel/python"
        },
        "detiknews_api/main.py": {
            "runtime": "@vercel/python"
        },
        "gateway/server.js": {
            "runtime": "@vercel/node"
        },
        "api-berita-indonesia/src/server.js": {
            "runtime": "@vercel/node"
        }
    },
    "rewrites": [
        {
            "source": "/cnn-api/:path*",
            "destination": "/cnnindonesia-news-api/main.py"
        },
        {
            "source": "/detik/:path*",
            "destination": "/detiknews_api/main.py"
        },
        {
            "source": "/rss/:path*",
            "destination": "/api-berita-indonesia/src/server.js"
        },
        {
            "source": "/berita-indo/:path*",
            "destination": "/berita-indo-api/package.json"
        },
        {
            "source": "/api/:path*",
            "destination": "/gateway/server.js"
        },
        {
            "source": "/",
            "destination": "/public/index.html"
        }
    ]
}